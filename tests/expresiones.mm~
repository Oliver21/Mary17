program polaco:
{	float ia;
	int ib;
	float ik;
	string ic;
	string id;
	string ie;
	bool bol;
	bol = 5!=6;
	ib = 7;
	ia = 4.3 * ib + 9 + 8 * 2 / 3 + 1 * 5 * (5 + 1 + 5 - 3) + 1;
	ic = "hola";
	id = "diego";
	ie = ic + id;
	print(ia);
	//ic = "Ya funciona el programa!!!. Genial";
	print (ie);
	print(bol);
	read();
		
}
